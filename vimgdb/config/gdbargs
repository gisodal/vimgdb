# pass arguments to simple python function
define somefunction
set $argv = 0
if $argc == 0
    set $argv = ""
end
if $argc == 1
    set $argv = "$arg0"
end
if $argc == 2
    set $argv = "$arg0@$arg1"
end

python
from __future__ import print_function
argv = str(gdb.parse_and_eval("$argv")).replace('"','').split('@')

print("argv in python:",argv)
end

